{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES5",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "module": "ES6",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src"]
}
